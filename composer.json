{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 9 projects with Composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.4",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/core-composer-scaffold": "^9.4",
        "drupal/core-recommended": "^9.4",
        "drupal/devel": "^4.1",
        "drush/drush": "^11.1",
        "vlucas/phpdotenv": "^5.1",
        "webflo/drupal-finder": "^1.2",
        "drupal/admin_toolbar": "^3.1",
        "drupal/coffee": "^1.2",
        "drupal/dotenv": "^1.0",
        "drupal/editor_md": "^4.0",
        "drupal/gin": "^3.0@beta",
        "drupal/gin_gutenberg": "^1.1",
        "drupal/gin_lb": "^1.0@beta",
        "drupal/gin_login": "^1.2",
        "drupal/gin_toolbar": "^1.0@beta",
        "drupal/gutenberg": "^2.5",
        "drupal/image_url_formatter": "^1.0",
        "drupal/layout_builder_component_attributes": "^2.1",
        "drupal/layout_builder_operation_link": "^2.0",
        "drupal/layout_builder_styles": "^1.0@beta",
        "drupal/layout_library": "^1.0@beta",
        "drupal/lb_direct_add": "^1.2",
        "drupal/markdown": "^3.0@RC",
        "drupal/paragraphs": "^1.14",
        "drupal/pathauto": "^1.10",
        "drupal/react_comments": "^1.0@beta",
        "drupal/redirect": "^1.7",
        "drupal/twig_tweak": "^3.1",
        "drupal/upgrade_status": "^3.13",
        "erusev/parsedown": "^1.7",
        "erusev/parsedown-extra": "^0.8.1",
        "league/commonmark": "^1.0",
        "michelf/php-markdown": "^1.9",
        "oomphinc/composer-installers-extender": "^1.1 || ^2"
    },
    "require-dev": {
        "drupal/core-dev": "^9.4",
        "mglaman/phpstan-drupal": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.6",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "discard-changes": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "files": ["load.environment.php"]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
        }
    }
}
